    <!-- Face registration Script -->
    <script>
        //Stream camera output into video element
        const video = document.getElementById("video");
        let allDetections = [];
        let noOfDetections = 2;


        function startVideo() {
            navigator.getUserMedia(
                { video: {} },
                (stream) => (video.srcObject = stream),
                (err) => console.error(err)
            );
        }
        //stop camera
        function stopCamera() {
            document.querySelector('.close').click();
            video.srcObject = null;
            video.src = '';
            video.pause();
            navigator.getUserMedia({ video: true }, (stream) => {
                stream.getTracks().forEach((track) => {
                    track.enabled = false;
                    track.stop();
                })
            }, err => console.error(err))
        }

        document.querySelector('.close').addEventListener('click', (evt) => {
            stopCamera();
        })
        document.getElementById('exampleModal').addEventListener('click', function (evt) {
            evt.stopPropagation();
            (evt.target == this) ? stopCamera() : '';
        })

        //require face models
        Promise.all([
            faceapi.nets.tinyFaceDetector.loadFromUri("/"),
            faceapi.nets.faceLandmark68Net.loadFromUri("/"),
            faceapi.nets.faceRecognitionNet.loadFromUri("/"),
            faceapi.nets.faceExpressionNet.loadFromUri("/"),
        ]).then(() => {
            //detect faces when camera turns on
            video.addEventListener("play", async () => {
                const canvas = faceapi.createCanvasFromMedia(video);
                canvas.classList.add("position-absolute");
                document.getElementById("wrapper").append(canvas);
                const displaySize = { width: video.width, height: video.height };
                faceapi.matchDimensions(canvas, displaySize);



                //We have to compile 4 angles of a single person's face. So we have to loop four times. Also we have to to add three faces from the front so we have to put a nested loop for the first iteration of outer loop
                let oneDetect;
                const allInputs = document.querySelectorAll('input');
                for (let checkbox of allInputs) {
                    (checkbox.type == 'checkbox') ? checkbox.addEventListener('click', (evt) => {
                        evt.preventDefault();
                    }) : '';
                }

                //Open loader ui
                await document.querySelector('.loader1').classList.replace('d-none', 'd-flex');
                const inverval2 = setInterval(async () => {
                    faceBoxStart(canvas, displaySize);
                }, 200)

                document.querySelector('.close').addEventListener('click', (evt) => {
                    canvas
                        .getContext("2d")
                        .clearRect(0, 0, canvas.width, canvas.height);
                    clearInterval(inverval2);
                })


                document.getElementById('next').addEventListener('click', async (evt) => {
                    document.getElementById('next').innerHTML = 'Next'
                    document.getElementById('detectionMessage').innerHTML = '';
                    if (allDetections.length == 3) {
                    }
                    //Open loader ui
                    await document.querySelector('.loader1').classList.replace('d-none', 'd-flex');

                    let currentDetection = [];
                    const interval = setInterval(async () => {
                        document.getElementById('detectionMessage').innerHTML = 'Detecting....';
                        oneDetect = await getDetection(canvas, displaySize);


                        if (oneDetect.length) {
                            currentDetection.push(oneDetect);
                        }
                        if (currentDetection.length > noOfDetections) {
                            clearInterval(interval);
                            canvas
                                .getContext("2d")
                                .clearRect(0, 0, canvas.width, canvas.height);
                            allDetections.push(currentDetection);
                            if (allDetections.length == 1) {
                                document.getElementById('clearDetection').classList.remove('d-none');
                                document.querySelector('.chk1').classList.remove('d-none');
                                document.getElementById('chk1').setAttribute('checked', true);
                                document.getElementById('detectionMessage').innerHTML = 'Front Detection completed.<br>Press next when ready.';
                                noOfDetections = 0;
                            }
                            else if (allDetections.length == 2) {
                                document.getElementById('chk2').setAttribute('checked', true);
                                document.getElementById('detectionMessage').innerHTML = 'Right Detection completed.<br>Press next when ready.';
                            }
                            else if (allDetections.length == 3) {
                                document.getElementById('chk3').setAttribute('checked', true);
                                document.getElementById('detectionMessage').innerHTML = 'Left Detection completed.<br>Press next when ready.';
                            }
                            else {
                                document.getElementById('chk4').setAttribute('checked', true);
                                document.getElementById('detectionMessage').innerHTML = '<h5 class="text-success">Your face models are successfully collected.<br> Press next to continue</h5>';
                                document.getElementById('next').classList.add('d-none');
                                document.getElementById('nextPro').classList.remove('d-none');
                                document.getElementById('nextPro').addEventListener('click', () => {
                                    document.querySelector('.close').click();
                                })
                            }
                        }
                    }, 1500);

                })


            });
        });


        async function getDetection(canvas, displaySize) {
            const detections = await faceapi
                .detectAllFaces(video, new faceapi.TinyFaceDetectorOptions())
                .withFaceLandmarks()
                .withFaceDescriptors();
            document.querySelector('.loader1').classList.replace('d-flex', 'd-none');

            return detections;
        }

        async function faceBoxStart(canvas, displaySize) {
            const detections = await faceapi
                .detectAllFaces(video, new faceapi.TinyFaceDetectorOptions({ minConfidence: 0.8 }))
            document.querySelector('.loader1').classList.replace('d-flex', 'd-none');
            const resizedDetections = faceapi.resizeResults(
                detections,
                displaySize
            );
            canvas
                .getContext("2d")
                .clearRect(0, 0, canvas.width, canvas.height);
            faceapi.draw.drawDetections(canvas, resizedDetections);
        }


        //When register button is clicked open camera and start detection
        const faceRegister = document.getElementById("registerFace");
        faceRegister.addEventListener("click", (evt) => {
            evt.preventDefault();
            evt.stopPropagation();
            document.getElementById('block-ui').click();
            startVideo();
        });

        //output to hold the image of unidentified person
        const outputImage = document.querySelector("img");
        // This function extract a face from video frame with giving bounding box and display result into outputimage
        async function extractFaceFromBox(inputVideo, box) {
            const regionsToExtract = [
                new faceapi.Rect(box.x, box.y, box.width, box.height),
            ];

            let faceImages = await faceapi.extractFaces(
                inputVideo,
                regionsToExtract
            );

            if (faceImages.length == 0) {
                console.log("Face not found");
            } else {
                faceImages.forEach((cnv) => {
                    outputImage.src = cnv.toDataURL();
                });
            }
        }

        document.getElementById('clearDetection').addEventListener('click', clearAllDetections)


        //Clear all detections
        async function clearAllDetections(evt) {
            const allInputs = document.querySelectorAll('input');
            for (let checkbox of allInputs) {
                checkbox.removeAttribute('checked');
            }
            this.classList.add('d-none');
            document.getElementById('detectionMessage').innerHTML = 'Click next to start detection again';
            allDetections = [];
            noOfDetections = 2;
            document.getElementById('next').classList.remove('d-none');
            document.getElementById('nextPro').classList.add('d-none');
        }


        // Generate Random Password
        document.getElementById('generatePass').addEventListener('click', (evt) => {
            //Generate random password
            document.getElementById('password').value = Math.floor(Math.random() * 1000000);
            document.getElementById('usernameField').innerHTML = document.getElementById('username').value;
            document.getElementById('passwordField').innerHTML = document.getElementById('password').value;


            if (!allDetections.length) {
                document.getElementById('alert-info').innerHTML = 'You Must register face of Student.';
                document.getElementById('registerFace').classList.add('border-danger');
                document.getElementById('alert').classList.remove('d-none');
                evt.preventDefault();
                evt.stopPropagation();
            }

            fillFormDetections();
            if (!document.forms[0].checkValidity()) {
                document.getElementById('alert-info').innerHTML = ' Input field(s) ';

                document.getElementById('alert').classList.remove('d-none');
                for (input of document.querySelectorAll('input')) {
                    if (input.type != 'file' && input.type != 'checkbox') {
                        if (input.value != 'student' && input.value != 'admin' && input.id != 'password') {
                            if (!input.checkValidity()) {
                                document.getElementById('alert-info').innerHTML += input.name + ', ';
                                input.classList.add('border-danger');
                            }
                        }
                    }
                }
                document.getElementById('alert-info').innerHTML += ' maybe left empty or filled inappropriatly';

                evt.preventDefault();
                evt.stopPropagation();
            }


            document.getElementById('submit').addEventListener('click', (evt) => {
                document.forms[0].submit();
            })
        })
        //Add data to form before submitting
        async function fillFormDetections(evt) {
            let descriptors = [];
            allDetections.map((value, i, arr) => {
                value.map((v, i, arr) => {
                    descriptors.push(v[0].descriptor);
                })
            })
            console.log("length of detection: " + descriptors.length);
            document.getElementById('descriptions').value = JSON.stringify(descriptors);
        }
    </script>

